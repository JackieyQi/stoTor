[program:stoTor]
command = /.virtualenv/bin/python /stoTor/main.py
directory=/stoTor
user=usero
autostart=true
autorestart=true
startsecs=10
startretries=3
redirect_stderr=true
;stdout_logfile=/var/log/sto.log
;stdout_logfile_maxbytes = 10MB
;logfile=/var/log/sto_log.log
logfile_maxbytes = 10MB
exitcodes=0,2
stopsignal=QUIT
